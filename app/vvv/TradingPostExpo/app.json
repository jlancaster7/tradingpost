{
  "expo": {
    "name": "TradingPostExpo",
    "slug": "TradingPostExpo",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#282869"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tradingpostapp",
      "associatedDomains": [
        "applinks:m.tradingpostapp.com"
      ]
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#282869"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "m.tradingpostapp.com",
              "pathPattern": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.tradingpostapp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "build": {
        "babel": {
          "include": [
            "@ui-kitten/components",
            "@tradingpost/common",
            "@ptomasroos/react-native-multi-slider"
          ]
        }
      }
    },
    "plugins": [
      "./plugins/withAndroidVerifiedLinksWorkaround",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo/android/logo_96.jpg",
          "color": "#282869"
        }
      ]
    ]
  }
}