  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npx expo start --web",
    "eject": "npx expo eject",
    "test": "jest --watchAll",
    "install-common": "node ../../../common/_scripts/install",
    "link-common": "node ../../../common/_scripts/link",
    "launch": "node ./_scripts/launch",
    "build-scripts": "tsc -b tsconfig.scripts.json",
    "pre-publish": "npm uninstall @tradingpost/common && npm install @tradingpost/common",
    "deploy": "npm uninstall @tradinpost/common && npm install @tradinpost/common && expo build:web && cd web-server && eb deploy"
