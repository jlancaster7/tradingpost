{
  "compilerOptions": {
    "target": "es2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "module": "CommonJS"
  }
}
